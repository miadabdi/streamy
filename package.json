{
	"name": "streamy",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "GPL-3.0-only",
	"scripts": {
		"minio:down:dev": "sudo docker compose rm minio -f -s",
		"minio:up:dev": "sudo docker compose up minio -d",
		"db:gen:migrate": "npx drizzle-kit generate:pg",
		"db:run:migrate": "tsx ./src/drizzle/migrate.ts",
		"db:up:dev": "sudo docker compose up streamy_db -d",
		"db:down:dev": "sudo docker compose down streamy_db",
		"db:rm:dev": "sudo docker compose rm streamy_db -f -s",
		"db:rebuild:dev": "npm run db:rm:dev && npm run db:up:dev && sleep 5 && npm run db:run:migrate",
		"db:up:test": "sudo docker compose up streamy_db_test -d",
		"db:down:test": "sudo docker compose down streamy_db_test",
		"db:rm:test": "sudo docker compose rm streamy_db_test -f -s",
		"db:rebuild:test": "npm run db:rm:test && npm run db:up:test && sleep 5 && npm run db:run:migrate",
		"build": "nest build",
		"format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"prepare": "husky"
	},
	"dependencies": {
		"@elastic/elasticsearch": "^8.15.0",
		"@nestjs-modules/mailer": "^2.0.2",
		"@nestjs/common": "^10.4.1",
		"@nestjs/config": "^3.2.3",
		"@nestjs/core": "^10.4.1",
		"@nestjs/elasticsearch": "^10.0.1",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.4.1",
		"@nestjs/swagger": "^7.4.0",
		"@nestjs/throttler": "^6.2.1",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.4",
		"argon2": "^0.41.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.4.5",
		"dotenv-expand": "^11.0.6",
		"drizzle-orm": "^0.31.4",
		"drizzle-zod": "^0.5.1",
		"helmet": "^7.1.0",
		"hpp": "^0.2.3",
		"joi": "^17.13.3",
		"minio": "^8.0.1",
		"moment": "^2.30.1",
		"multer": "^1.4.5-lts.1",
		"nest-winston": "^1.9.7",
		"nestjs-cls": "^4.4.1",
		"nestjs-minio-client": "^2.2.0",
		"nodemailer": "^6.9.15",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"pg": "^8.12.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"sharp": "^0.33.5",
		"uuid": "^10.0.0",
		"winston": "^3.14.2",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@nestjs/cli": "^10.4.5",
		"@nestjs/schematics": "^10.1.4",
		"@nestjs/testing": "^10.4.1",
		"@types/amqplib": "^0.10.5",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/hpp": "^0.2.6",
		"@types/jest": "^29.5.13",
		"@types/multer": "^1.4.12",
		"@types/node": "^20.16.5",
		"@types/nodemailer": "^6.4.15",
		"@types/passport-jwt": "^4.0.1",
		"@types/passport-local": "^1.0.38",
		"@types/pg": "^8.11.9",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^8.5.0",
		"@typescript-eslint/parser": "^8.5.0",
		"drizzle-kit": "^0.21.4",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.1",
		"typescript": "~5.6.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
